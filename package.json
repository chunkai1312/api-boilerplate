{
  "name": "punserve",
  "version": "0.0.0",
  "description": "A boilerplate for building api services based NodeJS, Express web framework and MongoDB",
  "main": "dist",
  "scripts": {
    "start": "node bin/api",
    "dev": "cross-env NODE_ENV=development nodemon bin/api",
    "build": "cross-env NODE_ENV=production babel src --out-dir dist --copy-files",
    "test": "cross-env NODE_ENV=test jest",
    "test:watch": "npm test -- --watch",
    "test:coverage": "npm test -- --coverage",
    "lint": "eslint src test"
  },
  "repository": "https://github.com/chunkai1312/punserve",
  "author": "Chun-Kai Wang <chunkai1312@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.17.2",
    "co-express": "^2.0.0",
    "compose-middleware": "^3.0.0",
    "compression": "^1.7.0",
    "consign": "^0.1.6",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.4",
    "dotenv": "^4.0.0",
    "express": "^4.15.3",
    "express-request-id": "^1.4.0",
    "express-winston": "^2.4.0",
    "helmet": "^3.8.1",
    "http-errors-express": "^3.1.0",
    "lodash": "^4.17.4",
    "method-override": "^2.3.9",
    "mongoose": "^4.11.5",
    "mongoose-delete": "^0.4.0",
    "mongoose-hidden": "^1.4.0",
    "morgan": "^1.8.2",
    "response-time": "^2.3.2",
    "winston": "^2.3.1",
    "winston-daily-rotate-file": "^1.4.6"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^20.0.3",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-env": "^1.6.0",
    "babel-register": "^6.24.1",
    "cross-env": "^5.0.2",
    "eslint": "^4.3.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^5.1.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "jest": "^20.0.4",
    "nodemon": "^1.11.0"
  }
}
